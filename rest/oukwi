User:
	email
	newsletter
	encrypted_password
	adresses [] : Address
	additional_info : Profile

Address:
	name
	surname
	address_line_1
	address_line_2
	postal_code
	city
	country : Country

Country:
	name

Voivodeship:
	name

Profile:
	first_name
	last_name
	date_of_birth
	telephone_number

Product:
	category : Category
	price
	name
	producer
	picture
	availability
	netto
	brutto

Delivery:
	name
	cost

PaymentMethod:
	name

Category:
	categories [] : Category - nested_set (left, right, parent_id, depth)
	name

Order:
	address : Address
	value
	order_positions [] : OrderPosition
	status
	deliveryCost
	delivery : Delivery
	paymentMethod : PaymentMethod

OrderPosition:
	product : Product
	price
	amount
	weight

Basket:
	user : User
	order_positions [] : OrderPosition
	status
